
<div class="books_index_body1">
	<div class="books_index_pickup">
	</div>
</div>
<header>
    <div id="header">
       <button id="button">MENU</button>
    </div>

  </header>
<div class="books_index_body2">
	
<div class="books_index_allbookmark">
	
	<div class="books_index_category">
			<h3>NEW BOOKMARK</h3>
		</div>
		<div class="books_index_bookmarks">
			<div class="books_index_bookmarks_box">
				<% @books.each do |book| %>
				<div class="books_index_bookmarks_box1">
					<%= link_to book_path(book.id) do %>
					<div class="books_index_bookmarks_image">
					</div>
					<% end %>
					<div class="books_index_bookmarks_name">
						<p><%= book.article %></p>
					</div>
					<div class="books_index_bookmark_category">
						<p><%= book.category %></p>
					</div>
					<div class="books_index_updated_at">
						<p><%= book.updated_at %></p>
					</div>
				</div>
				<% end %>
			</div>
		</div>
</div>
</div>
